<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>refit</id>
        <version>2.5.0-beta-002</version>
        <title>Refit</title>
        <authors>Paul Betts,Refit contributors</authors>
        <owners>Paul Betts</owners>
        <licenseUrl>https://github.com/xpaulbettsx/refit/blob/master/COPYING</licenseUrl>
        <projectUrl>https://github.com/xpaulbettsx/refit</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>The automatic type-safe REST library for Xamarin and .NET</description>
        <summary>The automatic type-safe REST library for Xamarin and .NET</summary>

        <dependencies>
          <group targetFramework="netstandard1.3">
            <dependency id="Microsoft.AspNetCore.WebUtilities" version="1.0.0" />
            <dependency id="NETStandard.Library" version="1.6.0" />
            <dependency id="Newtonsoft.Json" version="9.0.1" />
            <dependency id="System.Text.Encodings.Web" version="4.0.0" />
          </group>
            <group>
                <dependency id="Newtonsoft.Json" version="9.0.1" />
            </group>
            <group targetFramework="WindowsPhone8.0">
                <dependency id="Microsoft.Net.Http" version="2.2.29" />
                <dependency id="Microsoft.Bcl" version="1.1.10" />
                <dependency id="Newtonsoft.Json" version="9.0.1" />
            </group>
        </dependencies>
        <frameworkAssemblies>
            <frameworkAssembly assemblyName="System.Net.Http" targetFramework="portable-net45+monoandroid+xamarin.ios10" />
        </frameworkAssemblies>
    </metadata>
    <files>
        <file src="targets\*.*" target="build\portable-net45+netcore45+wp8+wpa81" />
        <file src="targets\*.*" target="build\netstandard1.3" />
        <file src="..\InterfaceStubGenerator\bin\Release\*.exe" target="tools" />
        <file src="..\InterfaceStubGenerator\bin\Release\*.dll" target="tools" />
        <file src="..\InterfaceStubGenerator\bin\Release\*.mustache" target="tools" />

        <file src="bin\Release\NetStandard1.3\Refit*.*" target="lib\netstandard1.3" />
        <file src="bin\Release\Net45\Refit*.*" target="lib\Net45" />
        <file src="bin\Release\Portable-Net45+WinRT45+WP8+WPA81\Refit*.*" target="lib\Portable-Net45+NetCore45+WP8+WPA81" />
        <file src="bin\Release\WP8\Refit*.*" target="lib\WP8" />
        <file src="bin\Release\Portable-Win81+Wpa81\Refit*.*" target="lib\Portable-Win81+Wpa81" />
        <file src="bin\Release\WinRT45\Refit*.*" target="lib\NetCore45" />
    </files>
</package>
